<nav class="nav">
  <div class="nav__inner">
    {{#each section}}
      <div class="nav__section">
        <a class="nav__section-title link" href=#{{href}}>{{title}}</a>
        {{#if list }}
          <ul class="nav__list">
            {{#each list}}
              <li class="nav__item">
                <a class="nav__link link" href=#{{title}}>{{title}}</a>
              </li>
            {{/each}}
          </ul>
        {{/if}}
      </div>
    {{/each}}
  </div>
</nav>
